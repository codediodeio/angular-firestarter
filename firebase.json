{
  "hosting": {
    "public": "dist/firestarter",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [{
      "source" : "/ngsw-worker.js",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "no-cache"
      }]
    }]
  },
  "firestore": {
    "rules": "firestore.rules"
  }
}
