{
  "name": "firestarter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test --watch false",
    "lint": "ng lint",
    "e2e": "cypress open",
    "build:all": "ng build --prod && ng run firestarter:server && npm run webpack:prerender && npm run prerender",
